{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "C_Source",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "R_C",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TransactionId as string,",
				"          TransactionDate as string,",
				"          OnlineRetailer as string,",
				"          SalesMonth as string,",
				"          SalesRegion as string,",
				"          SalesCurrency as string,",
				"          Title as string,",
				"          Vintage as string,",
				"          Variety as string,",
				"          Score as string,",
				"          ListPrice as string,",
				"          Quantity as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(left(TransactionDate,21) != \"Total Sales Quantity:\") ~> filter1",
				"derivedColumn1 aggregate(groupBy(OnlineRetailer,",
				"          SalesMonth,",
				"          TransactionDate,",
				"          SalesRegion,",
				"          SalesCurrency,",
				"          Title,",
				"          Vintage,",
				"          Variety,",
				"          Score),",
				"     Quantity = sum(toInteger(Quantity))) ~> aggregate1",
				"filter1 derive(FileName = concat(OnlineRetailer,'_',substring(SalesMonth,1, 4),'_',substring(SalesMonth,6,2))) ~> derivedColumn1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     rowUrlColumn:'SalesMonth',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}